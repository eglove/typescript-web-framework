{
  "compilerOptions": {
    "types": [
      "node"
    ],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noUnusedLocals": true,
    "pretty": true,
    "lib": [
      "esnext",
      "dom"
    ]
  }
}